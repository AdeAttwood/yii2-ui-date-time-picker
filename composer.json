{
    "name": "adeattwood/yii2-ui-date-time-picker",
    "description": "Yii 2 Date time picker using jquery ui and the time picker addon",
    "keywords": ["yii2", "date", "time", "picker", "widget"],
    "type": "yii2-extension",
    "authors": [
        {
            "name": "Ade Attwood",
            "email": "attwood16@googlemail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.5",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "bower-asset/jqueryui-timepicker-addon": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "adeattwood\\yii2\\datetimepicker\\": ""
        }
    },
    "config": {
        "process-timeout": 1800
    }
}
